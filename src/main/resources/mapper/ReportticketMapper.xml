<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyhc.sdglgroundconnection.mapper.ReportticketMapper">
    <select id="listReportticketById" resultType="com.dyhc.sdglgroundconnection.pojo.Reportticket">
        SELECT * FROM `reportticket` WHERE reportDetailId=#{reportDetailId} AND `status`=0
    </select>
    <select id="getMenPiaoById" resultType="com.dyhc.sdglgroundconnection.pojo.Reportticket">
        SELECT * FROM `reportticket` WHERE reportDetailId=#{reportDetailId} AND reportDate=#{liveDate}
    </select>
</mapper>