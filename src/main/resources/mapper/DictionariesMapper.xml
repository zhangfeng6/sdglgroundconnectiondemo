<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyhc.sdglgroundconnection.mapper.DictionariesMapper">
    <select id="listDictionaries" resultType="com.dyhc.sdglgroundconnection.pojo.Dictionaries">
        SELECT * FROM `dictionaries`
        WHERE typeCode='ATTRACTIONS'
    </select>

    <select id="selectById" resultType="com.dyhc.sdglgroundconnection.pojo.Dictionaries">
        SELECT * FROM `dictionaries`
        WHERE typeCode='DIET' and valueId=#{valueId}
    </select>
</mapper>